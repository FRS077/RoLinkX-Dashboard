www-data ALL=(ALL) NOPASSWD:/usr/bin/chmod guo+rw /sys/class/gpio/gpio[0-99]/value
www-data ALL=(ALL) NOPASSWD:/usr/bin/echo [0-9] > /sys/class/gpio/gpio[0-99]/value
www-data ALL=(ALL) NOPASSWD:/bin/systemctl start rolink.service
www-data ALL=(ALL) NOPASSWD:/bin/systemctl stop rolink.service
www-data ALL=(ALL) NOPASSWD:/bin/systemctl restart rolink.service
www-data ALL=(ALL) NOPASSWD:/bin/cat /etc/wpa_supplicant/wpa_supplicant.conf
